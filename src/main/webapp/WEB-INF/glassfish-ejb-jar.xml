<glassfish-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>ImplServiceUser</ejb-name>
      <jndi-name>co.com.acedwdev.sms.service.UserServiceWs</jndi-name>
      <webservice-endpoint>
        <port-component-name>ImplServiceUser</port-component-name>
        <login-config>
          <auth-method>BASIC</auth-method>
        </login-config>
      </webservice-endpoint>
    </ejb>
  </enterprise-beans>
</glassfish-ejb-jar>
